<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Product OData Service</display-name>

  <servlet>
    <servlet-name>ProductServlet</servlet-name>
    <servlet-class>com.nourology.odata.services.product.web.ProductServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>

  <servlet-mapping>
    <servlet-name>ProductServlet</servlet-name>
    <url-pattern>/ProductService.svc/*</url-pattern>
  </servlet-mapping>

</web-app>
